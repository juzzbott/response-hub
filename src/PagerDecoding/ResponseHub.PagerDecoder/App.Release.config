<?xml version="1.0"?>

<!-- For more information on using Web.config transformation visit http://go.microsoft.com/fwlink/?LinkId=301874 -->

<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
	<connectionStrings>
		<add name="MongoServer" connectionString="mongodb://localhost/response_hub" xdt:Transform="SetAttributes" xdt:Locator="Match(name)"/>
	</connectionStrings>

	<appSettings>
		<add key="LastMessageFile" value="F:\Response Hub Service\last-message" xdt:Transform="SetAttributes" xdt:Locator="Match(key)" />
	</appSettings>

	<logging logDirectory="F:\Response Hub Service\logs" logLevel="INFO" xdt:Transform="SetAttributes" />

	<mailTemplates templatesDir="F:\Response Hub Service" defaultTo="ResponseHub [info@responsehub.com.au]" defaultFrom="ResponseHub [info@responsehub.com.au]" xdt:Transform="SetAttributes" />

</configuration>
