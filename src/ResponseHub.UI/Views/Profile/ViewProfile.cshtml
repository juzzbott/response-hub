@model Enivate.ResponseHub.UI.Models.Profile.ProfileViewModel
@{
    ViewBag.Title = String.Format("{0} - Profile", Model.FullName);
}

<h1>@Model.FullName</h1>
<p class="text-info">Member number: @Model.Profile.MemberNumber</p>

<p>Member since: @Model.DateCreated.ToLocalTime().ToString("d MMMM, yyyy")</p>

