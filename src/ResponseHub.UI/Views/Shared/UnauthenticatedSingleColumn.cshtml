<!DOCTYPE html>
<html>
<head>
	<meta charset="utf-8" />
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<title>@ViewBag.Title - ResponseHub</title>

	<link rel="stylesheet" href="~/assets/css/framework.css" />
	<link rel="stylesheet" href="~/assets/css/response-hub.css" />

	<link href='https://fonts.googleapis.com/css?family=Ubuntu:400,300,300italic,700,700italic' rel='stylesheet' type='text/css'>

</head>
<body class="unauthenticated-single-layout">

	<div class="navbar navbar-default navbar-fixed-top page-navbar">
		<div class="container-fluid">
			<div class="navbar-header">
				<a href="/" class="navbar-brand">ResponseHub</a>
			</div>
		</div>
	</div>

	<div class="container body-content">

		<div class="row">

			<div class="col-sm-12 col-xl-8 col-xl-offset-2 main-content">
				<div class="content-container padding-bottom-20">
					@RenderBody()
				</div>
			</div>

		</div>

	</div>


	<script language="javascript" src="~/assets/js/framework.js"></script>
	<script language="javascript" src="~/assets/js/script.js"></script>

</body>
</html>