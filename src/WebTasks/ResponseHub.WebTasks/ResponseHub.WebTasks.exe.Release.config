<?xml version="1.0"?>
<!-- For more information on using Web.config transformation visit http://go.microsoft.com/fwlink/?LinkId=301874 -->
<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
	<connectionStrings>
		<add name="MongoServer" connectionString="mongodb://192.168.0.150/response_hub" xdt:Transform="SetAttributes" xdt:Locator="Match(name)"/>
	</connectionStrings>

	<appSettings>
		<add key="ServiceName" value="ResponseHub WebTasks" xdt:Transform="SetAttributes" xdt:Locator="Match(key)" />
	</appSettings>

	<weatherData radarCacheDirectory="E:\inetpub\responsehub.com.au\App_Data\Cache\BoM" xdt:Transform="SetAttributes(radarCacheDirectory)" />

	<logging logDirectory="E:\Response Hub WebTasks\logs" logLevel="INFO" xdt:Transform="SetAttributes" />

</configuration>